{
    "description": "Default renovatebot configuration with PR labeling for further PR selection",
    "extends": [
        "config:best-practices",
        ":disableDependencyDashboard",
        ":automergePr",
        ":automergeMinor",
        ":semanticCommitTypeAll(chore)",
        ":label(dependencies)"
    ],
    "addLabels": [
        "{{category}}",
        "{{datasource}}",
        "{{manager}}"
    ],
    "lockFileMaintenance": {
        "enabled": true
    }
}
